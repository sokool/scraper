{
  "url": "http://www.homegate.ch/",
  "storage": "xml",
  "graph": {
    "root": {
      "name": "buy",
      "selector": "li.page-nav-item--buy a",
      "neighbors": "sections"
    },
    "nodes": [
      {
        "name": "sections",
        "selector": "div.ad-aside-content div.box a",
        "neighbors": "countries"
      },
      {
        "name": "countries",
        "selector": "area[href]",
        "neighbors": "cantons"
      },
      {
        "name": "cantons",
        "selector": "area[href]",
        "neighbors": "documents, nextList"
      },
      {
        "name": "nextList",
        "selector": "a[rel=next]",
        "neighbors": "documents, nextList",
        "schema": "partial"
      },
      {
        "name": "documents",
        "selector": "a[class='detail-page-link box-row--link']",
        "schema": "details"
      }
    ]
  },
  "schemes": [
    {
      "test": "asd",
      "name": "details",
      "identifier": {
        "selector": "div.nr span",
        "action": "text",
        "filters": "trim, num",
        "validators": "notEmpty, isNum"
      },
      "structure": {
        "name": {
          "selector": "h1.title",
          "action": "text",
          "filters": "lower, trim, title"
        },
        "features": {
          "selector": "div.detail-key-data",
          "action": "list"
        }
      }
    },
    {
      "name": "partial",
      "identifier": {
        "selector": "div.nr span",
        "action": "text",
        "filters": "trim, num",
        "validators": "notEmpty, isNum"
      },
      "structure": {
      }
    }
  ]
}
